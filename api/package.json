{
  "name": "certs-api",
  "version": "0.1.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "local": "PORT=8080 yarn dev",
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "@docknetwork/sdk": "^0.3.9",
    "@hapi/iron": "^6.0.0",
    "@magic-sdk/admin": "^1.2.2",
    "aws-sdk": "^2.793.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie": "^0.4.1",
    "cors": "^2.7.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.13.3",
    "express-json-error-handler": "^4.0.0",
    "googleapis": "39",
    "magic-sdk": "^3.0.1",
    "mongoose": "^5.10.10",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme"
}
